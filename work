using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp26
{
    class Program
    {
        delegate double member(double x, int i, int now);

        delegate double funk(double x);

        delegate double now(int i);

        static double member1(double x, int i, int now)
        {
            return Math.Cos(i * x) / now;
        }
        static double member2(double x, int i, int now)
        {
            return Math.Pow(-1,now)*Math.Cos(now * x) / (now*now);
        }

        static double funk1(double x)
        {
            return Math.Exp(Math.Cos(x)) * Math.Cos(Math.Sin(x));
        }
        static double funk2(double x)
        {
            return ((x*x)-(Math.PI*Math.PI/3))/4;
        }



        static double S(int first, member member, funk funk, double a, double b, double h)
        {
            double S;
            bool flag;
            int i;
            for (double x = a; x <= b; x += h)
            {
                i = 1;
                flag = true;
                S = first;
                while(flag)
                {
                    S += member(x,i,)
                    i++;
                }
            }
        }
        static void Main(string[] args)
        {

        }  
    }
}
